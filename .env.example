# StuddyBuddy Environment Template
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to version control!

# Production Environment Variables
PORT=4000
JWT_SECRET=your-super-secret-jwt-key-here-make-it-long-and-random
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-app-password-here
GEMINI_API_KEY=your-gemini-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here

# Azure VM URLs (Update with your actual VM IP)
REACT_APP_BASE_URL=http://YOUR-VM-IP:4000
BACKEND_URL=http://YOUR-VM-IP:4000
FRONTEND_URL=http://YOUR-VM-IP

# Database Configuration (Using Docker PostgreSQL service)
POSTGRES_URI=postgresql://postgres:postgres@postgres:5432/postgres

# Docker Network Configuration
COMPOSE_PROJECT_NAME=studdybuddy

# Azure VM Configuration (Update with your actual IPs)
VM_PUBLIC_IP=YOUR-VM-PUBLIC-IP
VM_PRIVATE_IP=YOUR-VM-PRIVATE-IP
